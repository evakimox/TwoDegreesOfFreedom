    cent_x = room_width/2;
    cent_y = room_height/2;
    
    //labyrinth layers radius
    radius_1 = cent_y*0.9;   
    radius_list[0] = radius_1;
    for(i=0;i&lt;9;i++){
        radius_list[i+1]=radius_list[i]-cent_y*0.085;
    }    
    
    //floor sprite size in diameter in px
    floor_spr_size = 16;

    
    //code down there
    for(i=0;i&lt;10;i++){
        circumference = 2*3.14*radius_list[i];
        unitNeeded = circumference/(floor_spr_size*0.59);
        unitDegree = 360/unitNeeded;
        for(j=0;j&lt;unitNeeded;j++){
            current_deg = unitDegree*j*3.14/180;
            FloorBlock = instance_create(cent_x+radius_list[i]*cos(current_deg),cent_y+radius_list[i]*sin(current_deg),obj_Flowerfloor);
        }
    }</code>
